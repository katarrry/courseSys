<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="265.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.laboras.FileInfoWindow">
   <children>
      <Label fx:id="id" layoutX="51.0" layoutY="39.0" text="File id" />
      <Label fx:id="title" layoutX="219.0" layoutY="34.0" text="File Title">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="dateCreated" layoutX="49.0" layoutY="80.0" text="Date created" />
      <Label fx:id="dateUpdated" layoutX="47.0" layoutY="115.0" text="Date updated" />
      <Button fx:id="saveTitle" layoutX="232.0" layoutY="164.0" mnemonicParsing="false" onAction="#updateFileTitle" text="Save" />
      <TextField fx:id="newTitle" layoutX="46.0" layoutY="164.0" promptText="New file title" />
      <Button fx:id="deleteFile" layoutX="232.0" layoutY="214.0" mnemonicParsing="false" onAction="#deleteFile" text="Delete File" />
      <Button fx:id="back" layoutX="41.0" layoutY="214.0" mnemonicParsing="false" onAction="#returnToCourse" text="Back" />
   </children>
</AnchorPane>
